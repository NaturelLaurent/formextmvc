<?xml version="1.0" encoding="UTF-8"?>

<phpunit colors="true">
    <testsuite name="FE test unitaire">
        <directory>test/</directory>
    </testsuite>
    <logging>
        <log type="coverage-html" target="cover/" lowUpperBound="35"
             highLowerBound="70"/>
    </logging>

    <filter>
        <whitelist proceessUncoveredFromWhitelist="true">
            <directory suffix=".php">src/</directory>
        </whitelist>
    </filter>
</phpunit>